{
  "kind": "collectionType",
  "collectionName": "profiles",
  "info": {
    "singularName": "profile",
    "pluralName": "profiles",
    "displayName": "Profile",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "wallet_address": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "region": {
      "type": "enumeration",
      "enum": [
        "Europe",
        "NA",
        "Asia",
        "Oceania"
      ],
      "required": false
    },
    "username": {
      "type": "string",
      "unique": true,
      "maxLength": 20
    },
    "avatar": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "wager_mode": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "trust_mode": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "bio": {
      "type": "text",
      "required": false
    },
    "team_profiles": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::team-profile.team-profile",
      "mappedBy": "profile"
    },
    "notifications": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::notification.notification",
      "mappedBy": "profile"
    }
  }
}
