{
  "kind": "collectionType",
  "collectionName": "transactions",
  "info": {
    "singularName": "transaction",
    "pluralName": "transactions",
    "displayName": "Transaction",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "confirmed": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "deposit",
        "withdraw",
        "in",
        "out"
      ],
      "required": true
    },
    "amount": {
      "type": "integer",
      "required": true
    },
    "profile": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::profile.profile",
      "inversedBy": "transactions"
    },
    "txHash": {
      "type": "string",
      "unique": true,
      "required": false
    },
    "txBlockNumber": {
      "type": "biginteger"
    },
    "allowanceTxBlockNumber": {
      "type": "biginteger"
    },
    "onChainSinceBlockNumber": {
      "type": "biginteger"
    }
  }
}
